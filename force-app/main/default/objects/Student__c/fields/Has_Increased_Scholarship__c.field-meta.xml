<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Has_Increased_Scholarship__c</fullName>
    <description>Indicates whether a student was awarded an increased scholarship based on the Scholarship field.</description>
    <externalId>false</externalId>
    <formula>IF(TEXT(Scholarship__c) = &quot;Regular&quot; || TEXT(Scholarship__c) = &quot;Increased&quot; || TEXT(Scholarship__c) = &quot;Academic&quot;, TRUE, FALSE)</formula>
    <label>Has Increased Scholarship</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
